<%= form_for [current_user, @client_profile] do |f| %>
  <% if @client_profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@client_profile.errors.count, "error") %> prohibited this profile from being saved:</h2>

      <ul>
      <% @client_profile.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :phone_number %><br>
    <%= f.text_field :phone_number, :class => "form-control" %>
  </div>
  <div class="field">
    <%= f.label :city %><br>
    <%= f.text_field :city, :class => "form-control" %>
  </div>
  <div class="field">
    <%= f.label :state %><br>
    <%= f.text_field :state, :class => "form-control" %>
  </div>
  <div class="field">
    <%= f.label :street %><br>
    <%= f.text_field :street, :class => "form-control" %>
  </div>
  
	<!-- <button type="submit" class="btn btn-primary">Submit</button> -->
	<div class="actions">
	  <%= f.submit "Submit", class: "btn btn-primary" %>
	</div>
	
	
<% end %>