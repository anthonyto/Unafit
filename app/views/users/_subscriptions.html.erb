<div class="row">
	<div class="col-md-12">
		<div class="table-responsive">
			<table class="table table-striped table-hover">
				<tr>
					<th>Gym Name</th>
					<th>Street</th>
					<th>State</th>
					<th>Description</th>
					<th>Sessions left</th>
				</tr>
				<% @subscriptions.each do |subscription| %>
				<tr>
					<td><%= link_to subscription.gym.name, gym_path(subscription.gym.id), :class => 'navbar-link' %></td>
					<td><%= subscription.gym.street %></td>
					<td><%= subscription.gym.state %></td>
					<td><%= subscription.gym.description %></td>
					<td><%= subscription.sessions_left%></td>
				</tr>
			<% end %>
			</table>
		</div>
		<%= link_to "Update gym subscriptions", edit_subscriptions_user_path(current_user.id), :class => "btn btn-primary btn-sm" %>
	</div>
</div>